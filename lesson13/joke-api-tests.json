		{
			"name": "Get Ten Jokes",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response is an array\", function () {\r",
							"    const response = pm.response.json();\r",
							"    pm.expect(response).to.be.an('array');\r",
							"});\r",
							"\r",
							"pm.test(\"Has exactly 10 jokes\", function () {\r",
							"    const response = pm.response.json();\r",
							"    pm.expect(response).to.have.lengthOf(10);\r",
							"});\r",
							"\r",
							"pm.test(\"Each joke has correct structure\", function () {\r",
							"    const response = pm.response.json();\r",
							"    response.forEach(joke => {\r",
							"        pm.expect(joke).to.have.property('id');\r",
							"        pm.expect(joke).to.have.property('type');\r",
							"        pm.expect(joke).to.have.property('setup');\r",
							"        pm.expect(joke).to.have.property('punchline');\r",
							"    });\r",
							"});"
						],
