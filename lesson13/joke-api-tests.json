	"item": [
		{
			"name": "Get Random Joke",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response has joke structure\", function () {\r",
							"    const response = pm.response.json();\r",
							"    pm.expect(response).to.have.property('id');\r",
							"    pm.expect(response).to.have.property('type');\r",
							"    pm.expect(response).to.have.property('setup');\r",
							"    pm.expect(response).to.have.property('punchline');\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"Punchline is not empty\", function () {\r",
							"    const response = pm.response.json();\r",
							"    pm.expect(response.punchline).to.not.be.empty;\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://localhost:3005/jokes/random",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "3005",
					"path": [
						"jokes",
						"random"
					]
				}
			},
			"response": []
